    "import numpy as np\n",
    "\n",
    "a = np.arange(12).reshape(3, 4)\n",
    "\n",
    "b = np.arange(4).reshape(1, 4)\n",
    "\n",
    "print(a)\n",
    "print(b)\n",
    "\n",
    "# Broadcasts (a < 5, a, and b * 10)\n",
    "# of shape (3, 4), (3, 4) and (1, 4)\n",
    "c = np.where(a < 5, a, b * 10)\n",
    "\n",
    "print(c)"
